                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/tv_nombreSistema"
                android:ems="10"
                android:hint="Escribe Nombre del Sistema" />

            <TextView
