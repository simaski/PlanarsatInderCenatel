                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/tv_capacidadObraConduccion"
                    android:ems="10"
                    android:hint="Escriba Capacidad"
                    android:layout_weight="3"/>

